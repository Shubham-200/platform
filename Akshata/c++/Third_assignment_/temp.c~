if (month == (1 || 3 || 5 || 7 || 8 || 10 || 12) {
	if (day > 0 && day <= 31)
	cout << "January " << day << ", " << year << endl;
	else
	throw invalidDay("invalid day", day);}

	else
	if (month == 2) {
	if (day > 0 && day < 29)
	cout << "February " << day << ", " << year << endl;
	else
	if (day == 29) {
	if ((year % 400 == 0) || (year % 4 == 0) || (!(year % 100) == 0)) {
	throw invalidDay("noy a leap year", year);}
	else
	cout << "February " << day << ", " << year << endl;}
	else
	throw invalidDay("invalid Day", day);}
